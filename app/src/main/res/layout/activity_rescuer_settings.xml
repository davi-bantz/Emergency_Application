<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/userprof2"
    tools:context=".RescuerProfile.Rescuer_SettingsActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/emergency_contact"
        android:fontFamily="@font/archivo_black"
        android:text="USER SETTINGS"
        android:textAlignment="center"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/RescuerProfile"
        android:layout_width="336dp"
        android:layout_height="50dp"
        android:background="@drawable/emergency_contact"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/textView_userProfile"
        android:layout_width="189dp"
        android:layout_height="25dp"
        android:fontFamily="@font/archivo_black"
        android:text="User Profile"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_marginLeft="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerProfile"
        app:layout_constraintStart_toEndOf="@+id/ProfileIcon"
        app:layout_constraintTop_toTopOf="@+id/RescuerProfile" />


    <ImageButton
        android:id="@+id/imageButtonforRescuerProfile"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="UserProfilenavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerProfile"
        app:layout_constraintEnd_toEndOf="@+id/RescuerProfile"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toEndOf="@+id/textView_userProfile"
        app:layout_constraintTop_toTopOf="@+id/RescuerProfile"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_navigate_next_24" />

    <TextView
        android:id="@+id/RescuerServiceProfileUpdate"
        android:layout_width="336dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/emergency_contact"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RescuerProfile" />

    <ImageView
        android:id="@+id/ProfileIcon"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerProfile"
        app:layout_constraintStart_toStartOf="@+id/RescuerProfile"
        app:layout_constraintTop_toTopOf="@+id/RescuerProfile"
        app:srcCompat="@drawable/user_username" />

    <ImageButton
        android:id="@+id/imageButtonforRescuerService_Profile"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="EmergencyContactnavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintEnd_toEndOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintStart_toEndOf="@+id/Show_Res_Service"
        app:layout_constraintTop_toTopOf="@+id/RescuerServiceProfileUpdate"
        app:srcCompat="@drawable/baseline_navigate_next_24" />


    <TextView
        android:id="@+id/Show_Res_Service"
        android:layout_width="200dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/archivo_black"
        android:text="Service Profile"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintEnd_toEndOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintHorizontal_bias="0.012"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintVertical_bias="0.166" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintStart_toStartOf="@+id/RescuerServiceProfileUpdate"
        app:layout_constraintTop_toTopOf="@+id/RescuerServiceProfileUpdate"
        app:srcCompat="@drawable/user_location" />

    <ProgressBar
        android:id="@+id/progressBarEditProfile"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>




</androidx.constraintlayout.widget.ConstraintLayout>